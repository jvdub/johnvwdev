{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "react/no-unknown-property": [
      "error",
      {
        "ignore": ["dangerouslySetInnerHTML"]
      }
    ],
    "jsx-a11y/anchor-has-content": "warn",
    "jsx-a11y/anchor-is-valid": [
      "warn",
      {
        "components": ["Link"],
        "specialLink": ["to", "href"],
        "aspects": ["noHref", "invalidHref", "preferButton"]
      }
    ]
  }
}
